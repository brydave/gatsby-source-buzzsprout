    "@babel/core": "^7.4.5",
    "babel-preset-gatsby-package": "^0.1.4",
    "cross-env": "^5.2.0",
    "prettier": "^1.17.1"
  },
  "peerDependencies": {
    "gatsby": "^2.0.0"
  },
  "repository": {
